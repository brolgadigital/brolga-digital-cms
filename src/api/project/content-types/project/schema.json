    },
    "caseStudy": {
      "type": "richtext"
    },
    "overviews": {
      "type": "component",
      "repeatable": true,
      "component": "portfolio.service-type"
    },
    "launchDate": {
      "type": "date"
    },
    "subtitle": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "interestDescription": {
      "type": "text"
    },
    "review": {
      "type": "component",
      "repeatable": false,
      "component": "portfolio.review"
    },
    "coverImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": false
    }
